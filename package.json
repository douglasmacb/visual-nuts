{
  "name": "visual-nutes",
  "version": "1.0.0",
  "description": "Disruptive mindset. Business focused",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "nodemon src/main/server.ts",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests ./src/**/**/*",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "Douglas Miranda <douglasmacbrito@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.15",
    "sucrase": "^3.14.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
